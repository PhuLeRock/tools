
yum install ncurses-libs
yum install ncurses-devel
ls
rm -rf CMakeCache.txt
/usr/local/bin/cmake . -DCMAKE_INSTALL_PREFIX:PATH=/usr/local/mysql -DCOMMUNITY_BUILD:BOOL=ON -DENABLED_PROFILING:BOOL=ON -DENABLE_DEBUG_SYNC:BOOL=OFF -DINSTALL_LAYOUT:STRING=STANDALONE -DMYSQL_DATADIR:PATH=/data/mysqldata -DMYSQL_MAINTAINER_MODE:BOOL=OFF -DWITH_EMBEDDED_SERVER:BOOL=ON -DWITH_EXTRA_CHARSETS:STRING=all -DWITH_SSL:STRING=bundled -DWITH_UNIT_TESTS:BOOL=OFF -DWITH_ZLIB:STRING=bundled -DWITH_INNOBASE_STORAGE_ENGINE=1   -DWITH_COMMENT=dancebear -DDEFAULT_CHARSET=utf8 -DDEFAULT_COLLATION=utf8_general_ci -LH
make


